         TITLE 'P D S  --  PDS DSNAME                        09/28/88'  00010000
*********************************************************************** 00020000
***      DSNAME  SUBCOMMAND                                         *** 00030000
*********************************************************************** 00040000
*                                                                       00050000
* THIS ROUTINE IS CALLED BY BROWSE, CHANGE, FIXPDS, AND USAGE:          00051037
*                                                                       00052035
*        L     R15,=A(DSNAMES)                                          00052136
*        BASR  R2,R15                                                   00053036
*                                                                       00054036
         SPACE 1                                                        00060000
DSNAMES  CSECT                                                          00070038
         BASR  R6,0                                                     00070134
         USING *,R6                                                     00070234
         SPACE 1                                                        00071030
         CLC   ##SUBCOM(8),$DSN         DSN SUBCOMMAND?                 00100000
         BNE   DSNAMES2                 NO, BRANCH                      00110000
         TM    DS1SMSFG,DS1PDSE         PDSE DATASET?         DRK AUG05 00110115
         BO    DSNAMES2                 YES                   DRK AUG05 00111015
*** REOPEN THE DATA SET TO ENSURE DS1LSTAR IS CORRECT                   00120000
         OI    FLAGSJJ,FNOREAD          CALL EXCP FOR OPEN ONLY         00130000
         MVC   STARTTR(3),DS1LSTAR      SET BEYOND END OF DATA SET      00140000
         L     R15,=V(EXCP)                                             00150000
         BASR  R14,R15                                                  00160000
         NI    FLAGSJJ,FF-FNOREAD       CALL IS COMPLETED               00170000
         SPACE 2                                                        00180000
DSNAMES2 DS    0H                                                       00190000
         MVC   INSERT#1(80),BLANK128    BLANK THE FIRST LINE            00200000
         MVI   MTLEN,80                 80 CHARACTER MESSAGE            00210000
         TM    FLAGSJJ,FDSNMSG          MSG ALLOCATION?                 00220000
         BO    DSNMSG                   YES, BRANCH                     00230000
         TM    FLAGSJJ,FDSNTSO          TSO ALLOCATION?                 00240000
         BO    DSNTSO                   YES, BRANCH                     00250000
         SPACE 1                                                        00260000
DSNJCL   MVC   INSERT#1(20),DSNJDSN     ADD "//12345678  DD  DSN="      00270000
         MVC   INSERT#1+2(8),DDNAME     MOVE IN THE DDNAME              00280000
         LA    R1,INSERT#1+20           DSNAME START POSITION           00290000
         MVC   0(44,R1),DSNAME          MOVE IN THE DSNAME              00300000
         AH    R1,DSNLEN                LENGTH OF THE DSNAME            00310000
         MVC   0(15,R1),DSNJDSND        ADD ",DISP=SHR,UNIT="           00320000
         CLI   DSPALLOC,ALLOSHR         DISP=SHR?                       00330000
         BE    *+10                     YES, BRANCH                     00340000
         MVC   6(3,R1),=C'OLD'          NO, DISP=OLD                    00350000
         LH    R15,BYTEUCB              UCBBYTE                         00360000
         MH    R15,=H'9'                INDEX INTO THE UNIT TABLE       00370000
         LA    R15,UNITTBL(R15)         DEVICE UNIT TYPE                00380000
         LA    R1,15(,R1)               START OF UNIT DATA              00390000
         MVC   0(8,R1),1(R15)           ADD TO THE MESSAGE              00400000
         MVI   8(R1),X'40'              ADD A TERMINATOR                00410000
         LA    R1,1(,R1)                SCAN                            00420000
         CLI   0(R1),X'40'                  FOR FIRST                   00430000
         BNE   *-8                                   BLANK              00440000
         MVI   0(R1),C','               ADD A TERMINATOR AND CONTINUE   00450000
         M$MSG L220$1                                                   00460000
         MVC   INSERT#1(80),BLANK128    BLANK THE SECOND LINE           00470000
         MVC   INSERT#1(20),DSNJDCT     "// DCB=(RECFM=, ..."           00480000
         IC    R5,FLAGSCC               RECFM BITS                      00490000
         SLL   R5,29                    DROP TOP BITS                   00500000
         SRL   R5,29                    REPOSITION                      00510000
         IC    R5,RECFMTYP(R5)          RECORD FORMAT                   00520000
         STC   R5,INSERT#1+15           MOVE INTO MESSAGE               00530000
         LA    R1,INSERT#1+16                                           00540000
         TM    RECFM,DCBRECBR           RECFM=.B?                       00550000
         BNO   *+12                     NO, BRANCH                      00560000
         MVI   0(R1),C'B'                                               00570000
         LA    R1,1(R1)                                                 00580000
         TM    RECFM,DCBRECSB           RECFM=.S?                       00590000
         BNO   *+12                     NO, BRANCH                      00600000
         MVI   0(R1),C'S'                                               00610000
         LA    R1,1(R1)                                                 00620000
         TM    RECFM,DCBRECTO           RECFM=.T?                       00630000
         BNO   *+12                     NO, BRANCH                      00640000
         MVI   0(R1),C'T'                                               00650000
         LA    R1,1(R1)                                                 00660000
         TM    RECFM,DCBRECCA           RECFM=.A?                       00670000
         BNO   *+12                     NO, BRANCH                      00680000
         MVI   0(R1),C'A'                                               00690000
         LA    R1,1(R1)                                                 00700000
         TM    RECFM,DCBRECCM           RECFM=.M?                       00710000
         BNO   *+12                     NO, BRANCH                      00720000
         MVI   0(R1),C'M'                                               00730000
         LA    R1,1(R1)                                                 00740000
         TM    FLAGSCC,RECFMU           RECFM=U?                        00750000
         BO    DSN010                   YES, NO LRECL                   00760000
         MVC   0(7,R1),DSNJDCTL         MOVE IN LRECL TEXT              00770000
         LA    R1,7(R1)                                                 00780000
         LH    R4,LABLRECL                                              00790000
         BAS   R14,DSNCONV                                              00800000
         SPACE 1                                                        00810000
DSN010   MVC   0(9,R1),DSNJDCTB                                         00820000
         LA    R1,9(R1)                                                 00830000
         LH    R4,BLKSI                                                 00840000
         BAS   R14,DSNCONV                                              00850000
         SPACE 1                                                        00860000
         SR    R4,R4                                                    00870000
         ICM   R4,B'0001',DS1KEYL       ANY KEY LENGTH?                 00880000
         BZ    DSN020                   NO, BRANCH                      00890000
         MVC   0(8,R1),DSNJDCTK         ADD ,KEYLEN=                    00900000
         LA    R1,8(R1)                                                 00910000
         BAS   R14,DSNCONV                                              00920000
         SPACE 1                                                        00930000
         MVC   0(5,R1),DSNJDCTR         ADD ,RKP=                       00940000
         LA    R1,5(R1)                                                 00950000
         SR    R4,R4                                                    00960000
         ICM   R4,B'0011',DS1RKP        RELATIVE KEY POSITION           00970000
         BAS   R14,DSNCONV                                              00980000
         SPACE 1                                                        00990000
DSN020   TM    OPTCD,X'A4'              ANY OPTCD CHARACTERS?           01000000
         BZ    DSN024                   NO, BRANCH                      01010000
         MVC   0(7,R1),DSNJDCTO         ADD ,OPTCD=                     01020000
         LA    R1,7(R1)                                                 01030000
         TM    OPTCD,DCBOPTW            OPTCD=W?                        01040000
         BNO   *+12                     NO, BRANCH                      01050000
         MVI   0(R1),C'W'                                               01060000
         LA    R1,1(R1)                                                 01070000
         TM    OPTCD,DCBOPTC            OPTCD=C?                        01080000
         BNO   *+12                     NO, BRANCH                      01090000
         MVI   0(R1),C'C'                                               01100000
         LA    R1,1(R1)                                                 01110000
         TM    OPTCD,DCBSRCHD           OPTCD=Z?                        01120000
         BNO   *+12                     NO, BRANCH                      01130000
         MVI   0(R1),C'Z'                                               01140000
         LA    R1,1(R1)                                                 01150000
         SPACE 1                                                        01160000
DSN024   TM    DSORG,DS1DSGPO           PARTITIONED?                    01170000
         BO    DSN030                   NO, BRANCH                      01180000
         MVC   0(7,R1),DSNJDCTD         ADD ,DSORG=                     01190000
         IC    R5,DSORG                 DSORG BITS                      01200000
         SLL   R5,24                    DROP TOP BITS                   01210000
         SRL   R5,28                    REPOSITION BITS *2              01220000
         LA    R5,DSORGTYP(R5)          DSORG                           01230000
         MVC   7(2,R1),0(R5)            MOVE INTO MESSAGE               01240000
         TM    DSORG+1,DS1ACBM          VSAM DATA SET?                  01250000
         BNO   *+10                     YES, BRANCH                     01260000
         MVC   7(2,R1),DSNLVS                                           01270000
         LA    R1,9(,R1)                SKIP OVER DSORG TEXT            01280000
         TM    DS1DSORG,DS1DSGU         UNMOVEABLE?                     01290000
         BNO   DSN030                   NO, BRANCH                      01300000
         MVI   0(R1),C'U'               ADD U                           01310000
         LA    R1,1(,R1)                SKIP OVER U                     01320000
         SPACE 1                                                        01330000
DSN030   MVC   0(10,R1),DSNJDCTV        ADD ),VOL=SER=                  01340000
         MVC   10(6,R1),VOLALLOC        ADD THE SERIAL NUMBER           01350000
         MVI   10+6(R1),C','                                            01360000
         M$MSG L220$1                                                   01370000
         SPACE 1                                                        01380000
         MVC   INSERT#1(80),DSNJSPA     //  SPACE= ...                  01390000
         L     R1,DCBDEBAD-IHADCB+INDCB GET DEB ADDRESS                 01400000
         LH    R14,NUMEXT               NUMBER OF EXTENTS               01410000
         SR    R0,R0                                                    01420000
         SR    R15,R15                                                  01430000
*                                                                       01440000
         ICM   R15,3,32+14(R1) <----+   NUMBER OF TRACKS IN EXTENT      01450000
         AR    R0,R15               |   ADD NUMBER OF TRACKS            01460000
         LA    R1,16(,R1)           |   NEXT EXTENT                     01470000
         BCT   R14,*-4-2-4  --------+   BRANCH FOR ALL EXTENTS          01480000
*                                                                       01490000
*        A     R0,=F'100000'            FAT DASD TEST         DRK MAY98 01491002
         ST    R0,DSNTOTAL              SAVE TOTAL D.S. SIZE IN TRACKS  01500000
         SR    R1,R1                                                    01510000
         ICM   R1,3,DS1LSTAR            GET TT OF LAST TRACK            01520000
         CLC   DS1LSTAR(3),ZERO         ANY ALLOCATION?                 01530000
         BE    *+8                      NO, BRANCH                      01540000
         LA    R1,1(,R1)                JUMP FOR COMPUTATION            01550000
         SR    R0,R1                                                    01560000
         ST    R0,DSNEMPTY              TOTAL FREE SPACE                01570000
         SPACE 1                                                        01580000
         LA    R1,INSERT#1+DSNJSPAP     PRIMARY SPACE                   01590000
         L     R4,DSNTOTAL              TOTAL FREE SPACE                01600000
         TM    DS1SCALO,X'C0'           CYLINDER ALLOCATION?            01610000
         BNO   DSN050                   NO, BRANCH                      01620000
         MVC   INSERT#1+11(3),=C'CYL'   YES, MARK AS CYLINDER ALLOC.    01630000
         LH    R0,DEVTTRKS              TRACKS/CYLINDER                 01640000
         LR    R5,R0                                                    01650000
         BCTR  R5,0                     TRACKS/CYLINDER -1              01660000
         AR    R5,R4                    TOTAL+TRK/CYL -1                01670000
         SR    R4,R4                                                    01680000
         DR    R4,R0                    CEIL(TOTAL+TRK/CYL-1) / TRK/CYL 01690000
         LR    R4,R5                    RESULTING QUOTIENT              01700000
DSN050   BAS   R14,DSNCONV                                              01710000
         SPACE 1                                                        01720000
         SR    R4,R4                                                    01730000
         ICM   R4,B'0111',DS1SCALO+1    SECONDARY ALLOCATION?           01740000
         BZ    DSN070                   NO, BRANCH                      01750000
         TM    DS1SCALO,X'80'           TRACK OR CYLINDER ALLOCATION?   01760000
         BO    DSN060                   YES, BRANCH                     01770000
         TM    DS1SCALO,X'40'           AVERAGE BLOCK ALLOCATION?       01780000
         BNO   DSN060                   NO, BRANCH                      01790000
         LH    R14,BLKSI                BLOCKSIZE                       01800000
         ICM   R14,B'1000',=X'01'       R=1, K=0, DD=BLKSIZE            01810000
         PUSH  ACONTROL                                       DRK MAY08 01810142
         ACONTROL FLAG(NOPAGE0)                               DRK MAY08 01810241
         TRKCALC FUNCTN=TRKCAP,TYPE=BYTEUCB+1,RKDD=(14),               X01820000
               REGSAVE=YES,MF=(E,PARMLIST)                              01830000
         POP   ACONTROL                                       DRK MAY08 01831041
         LR    R5,R0                                                    01840000
         BCTR  R5,0                                                     01850000
         AR    R5,R4                                                    01860000
         SR    R4,R4                                                    01870000
         DR    R4,R0                    CEIL((BUFFERS+#/TRK-1)/(#/TRK)) 01880000
         LR    R4,R5                    RESULTING QUOTIENT              01890000
DSN060   MVI   0(R1),C','               SEPARATOR                       01900000
         LA    R1,1(,R1)                                                01910000
         BAS   R14,DSNCONV                                              01920000
         SPACE 1                                                        01930000
DSN070   TM    DSORG,DS1DSGPO           PARTITIONED DATA SET?           01940000
         BNO   DSN080                   NO, BRANCH                      01950000
         TM    DS1SMSFG,DS1PDSE         PDSE DATASET?         DRK NOV10 01951046
         BO    DSN080                   YES                   DRK NOV10 01952046
         MVI   0(R1),C','               SEPARATOR                       01960000
         CLC   DS1SCALO+1(3),ZERO       SECONDARY ALLOCATION?           01970000
         BNE   *+8                      YES, BRANCH                     01980000
         LA    R1,1(,R1)                NO, ADD ANOTHER COMMA           01990000
         MVI   0(R1),C','               SEPARATOR                       02000000
         LA    R1,1(,R1)                                                02010000
         L     R4,TOTALLOX              TOTAL DIRECTORY BLOCKS          02020000
         BAS   R14,DSNCONV                                              02030000
         SPACE 2                                                        02040000
DSN080   MVI   0(R1),C')'               PAIRED PARENTHESIS 1            02050000
         MVI   1(R1),C')'               PAIRED PARENTHESIS 2            02060000
         TM    DS1SCALO,X'01'           CYLINDER ROUNDING?              02070000
         BNO   *+10                     NO, BRANCH                      02080000
         MVC   1(9,R1),DSNLROUN         YES, MARK IT                    02090000
         SPACE 1                                                        02100000
         TM    DS1SMSFG,DS1PDSE         PDSE DATASET?         DRK NOV10 02100143
         BNO   DSN085                   NO                    DRK NOV10 02101043
         LA    R1,INSERT#1+DSNJSPAT     YES, WHERE TO STORE   DRK NOV10 02101143
         MVC   0(3,R1),=C'N/A'          NO FREE               DRK NOV10 02101243
         LA    R1,3(R1)                 BUMP                  DRK NOV10 02101344
         B     DSN090                                         DRK NOV10 02101443
DSN085   DS    0H                                                       02102043
         LA    R1,INSERT#1+DSNJSPAT     WHERE TO STORE NUMBER FREE      02110000
         L     R4,DSNEMPTY              TOTAL FREE TRACKS               02120000
         BAS   R14,DSNCONV                                              02130000
         SPACE 1                                                        02140000
         TM    DSORG,DS1DSGPO           PARTITIONED?                    02150000
         BNO   DSN090                   NO, BRANCH                      02160000
         MVC   0(10,R1),DSNLFREE        FREE DIRECTORY BLOCKS           02170000
         LA    R1,10(,R1)               WHERE TO STORE NUMBER FREE      02180000
         L     R4,TOTALLOX              TOTAL DIRECTORY BLOCKS          02190000
         S     R4,TOTUSEDX              FREE DIRECTORY BLOCKS           02200000
         BAS   R14,DSNCONV                                              02210000
DSN090   MVI   0(R1),C'*'               TERMINATOR                      02220000
         MVI   1(R1),C'/'                         CHARACTERS            02230000
         M$MSG L220$1                                                   02240000
         B     DSNFIN                                                   02250000
         SPACE 3                                                        02260000
DSNTSO   MVC   INSERT#1(22),DSNTDSN     ADD "ALLOC F(1234567)  DA('"    02270000
         MVC   INSERT#1+8(8),DDNAME     MOVE IN THE DDNAME              02280000
         LA    R1,INSERT#1+8            DDNAME START POSITION           02290000
         LA    R1,1(,R1)                SCAN                            02300000
         CLI   0(R1),X'40'                  FOR FIRST                   02310000
         BNE   *-8                                   BLANK              02320000
         MVI   0(R1),C')'               ADD TERMINATOR                  02330000
