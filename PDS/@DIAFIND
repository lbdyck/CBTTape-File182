         TITLE 'ISPF - FIND  FUNCTION'                         SS NOV85 00010000
* FIND SUPPORT                                                 SS MAY85 00020000
ISPFIND  DS    0H          ****    FIND  SUBCOMMAND  ****      SS MAY85 00030000
         USING *,R4                                            SS NOV85 00040000
         L     R15,SPFOPER           FIRST BLANK AFTER CMD     SS MAY85 00050000
         CLI   $TLEX(14),4           EX BYTE SET FOR RFIND     SS AUG86 00060000
         BNE   ISPFIND4              NO, CONTINUE              SS AUG86 00070000
         LA    R14,46                SCAN COUNT                SS AUG86 00080000
         BAS   R2,ISPCKBLK           FIND NON BLANK            SS AUG86 00090000
         LTR   R15,R15               ANY OPERANDS              SS AUG86 00100000
         BZ    ISPFLG03              NO STRING                 SS AUG86 00110000
         CLI   0(R15),C'F'           RFIND AND F TOGETHER      SS AUG86 00120000
         BE    ISPFIND5              YES, CHECK BLANK          SS AUG86 00130000
         CLI   0(R15),X'86'          LOW CASE F                SS AUG86 00140000
         BNE   ISPFIND4              NO, CONTINUE              SS AUG86 00150000
ISPFIND5 DS    0H                       *                      SS AUG86 00160000
         CLI   1(R15),C' '           F-BLANK                   SS AUG86 00170000
         BNE   ISPFIND4              NO, CONTINUE              SS AUG86 00180000
         LA    R15,1(R15)            SKIP F                    SS AUG86 00190000
ISPFIND4 DS    0H                                              SS AUG86 00200000
         LA    R14,46                TRY FOR 46 BYTES          SS MAY85 00210000
         LH    R1,SPFTBPFD         FIND INDEX CODE             SS NOV85 00220000
         L     R1,ISPFDPR(R1)      GET PARSE TABLE             SS NOV85 00230000
         BAS   R2,ISPPARSI           PARSE ROUTINE             SS NOV85 00240000
         C     R15,ISPL8             RETURN CODE               SS MAY85 00250000
         BH    ISPFLGX9                                        SS OCT88 00260000
         CLI   SPFPRRC,8           NO STRING                   SS MAY85 00270000
         BE    ISPFLG03                                        SS MAY85 00280000
         TM    SPFPRBI,X'03'       PREFIX OR SUFFIX            SS MAY85 00290000
         BO    ISPFLGX2            BOTH.. ERROR                SS MAY85 00300000
         TM    SPFPRBI,X'05'       WORD OR SUFFIX              SS MAY85 00310000
         BO    ISPFLGX2            BOTH.. ERROR                SS MAY85 00320000
         TM    SPFPRBI,X'06'       WORD OR PREFIX              SS MAY85 00330000
         BO    ISPFLGX2            BOTH.. ERROR                SS MAY85 00340000
         MVC   SPFTFF,SPFPRBI                                  SS NOV85 00350000
         NI    SPFTFF2,255-X'02'   CLEAR AT TOP/BOTTOM         SS NOV85 00360000
         NI    SPFTFF2,255-X'18'   CLEAR REPEAT FIND/SEC NUM   SS JAN86 00370000
         OI    SPFTFF2,X'01'       HAVE STRING                 SS NOV85 00380000
         TM    SPFPRBI2,X'80'      HAVE SECONDED NUMBER        SS NOV85 00390000
         BZ    *+8                                             SS NOV85 00400000
         OI    SPFTFF2,X'10'                                   SS NOV85 00410000
         NI    SPFPRBI2,255-X'80'  CLEAR                       SS NOV85 00420000
* SPFTFST IS SET TO START OF FIELD FOR SEARCH                  SS NOV85 00430000
* SPFTFED IS SET TO LENGTH OF FIELD                            SS NOV85 00440000
         LH    R1,SPFTBPFD         FIND INDEX CODE             SS NOV85 00450000
         LTR   R1,R1               INDEX 0                     SS NOV85 00460000
         BZ    ISPFINDS            YES, FOR SESSION LOG        SS NOV85 00470000
         L     R1,ISPFDCK(R1)      GET CHECK TABLE             SS NOV85 00480000
ISPFINDL CLI   0(R1),X'FF'         END OF TABLE                SS NOV85 00490000
         BE    ISPFLGX2            MUST BE KEY CONFLICT        SS NOV85 00500000
         CLC   0(2,R1),SPFPRBI2    COMPARE BYTES 2 AND 3       SS NOV85 00510000
         BE    ISPFINDF            GOT A MATCH                 SS NOV85 00520000
         LA    R1,06(,R1)          NEXT ENTRY                  SS NOV85 00530000
         B     ISPFINDL            LOOP                        SS NOV85 00540000
* THE FOLLOWING ARE FOR SESSION LOG                            SS NOV85 00550000
* OTHERS COULD BE SET FOR OTHER TABLES                         SS NOV85 00560000
ISPFINDS LA    R1,SPFLINE                                      SS NOV85 00570000
         ST    R1,SPFTFST          STARTING LOCATION           SS NOV85 00580000
         LA    R1,158                                          SS OCT88 00590000
         STH   R1,SPFTFED          SET LENGTH                  SS NOV85 00600000
         B     ISPFIND3                                        SS NOV85 00610000
* THIS IS END OF PARAMETERS FOR LOG FIND                       SS NOV85 00620022
ISPFINDF DS    0H                  GOOD ONE                    SS NOV85 00630000
         LH    R14,2(R1)           GET FIELD OFFSET            SS NOV85 00640000
         LA    R14,SPWRKC(R14)     CALC ADDRESS                SS NOV85 00650000
         ST    R14,SPFTFST         STARTING LOCATION           SS NOV85 00660023
         LH    R14,4(R1)           LENGTH                      SS NOV85 00670000
         STH   R14,SPFTFED         SET LENGTH                  SS NOV85 00680000
         B     ISPFIND3                                        SS NOV85 00690000
ISPFIND3 DS    0H                                              SS NOV85 00700000
         TM    SPFTFF,X'40'        OFFSET FLAG                 SS NOV85 00710000
         BZ    ISPFLG01            NO                          SS NOV85 00720000
         LH    R1,SPFTFED          LENGTH                      SS JAN86 00730000
         LA    R1,1(,R1)           BASE 1                      SS JAN86 00740000
         C     R1,SPFPRNU          CHECK RANGE                 SS JAN86 00750000
         BL    ISPFLGX1            RANGE ERROR                 SS JAN86 00760000
         TM    SPFTFF2,X'10'       ENDING RANGE ALSO           SS JAN86 00770000
         BZ    ISPFLGD1            SKIP SECOND RANGE CHECK     SS JAN86 00780000
         C     R1,SPFPRNU2         CHECK RANGE                 SS JAN86 00790000
         BL    ISPFLGX1            RANGE ERROR                 SS JAN86 00800000
         CLC   SPFPRNU,SPFPRNU2    SECOND MUST FOLLOW FIRST    SS JAN86 00810000
         BH    ISPFLGX1            RANGE ERROR                 SS JAN86 00820000
ISPFLGD1 DS    0H                                              SS JAN86 00830000
         BCTR  R1,0                BASE 0                      SS JAN86 00840000
         SH    R1,SPFPRLN          LAST POSSIBLE POSITION      SS JAN86 00850000
         BM    ISPFLGX3            LENGTH ERROR                SS JAN86 00860000
         L     R14,SPFPRNU         OFFSET                      SS JAN86 00870000
         BCTR  R14,0               REF TO ZERO                 SS JAN86 00880000
         CR    R14,R1              AFTER LAST POSSIBLE POSITIONSS JAN86 00890000
         BH    ISPFLGX3            YES, LENGTH ERROR           SS JAN86 00900000
         L     R15,SPFTFST         STARTING LOCATION           SS JAN86 00910000
         AR    R15,R14             ADD POSITION                SS JAN86 00920000
         ST    R15,SPFTFST         NEW STARTING LOCATION       SS JAN86 00930000
         TM    SPFTFF2,X'10'       ENDING RANGE                SS JAN86 00940000
         BZ    ISPFLGA1            NO                          SS JAN86 00950000
         L     R15,SPFPRNU2        GET IT                      SS JAN86 00960000
         BCTR  R15,0               REF TO ZERO                 SS JAN86 00970000
         CR    R15,R1              AFTER LAST POSSIBLE POSITIONSS JAN86 00980000
         BH    *+6                 YES                         SS JAN86 00990000
         LR    R1,R15              NO, USE THIS POSITION       SS JAN86 01000000
         SR    R1,R14              LAST MINUS FIRST            SS JAN86 01010000
         BZ    ISPFLGA1            NO LENGTH, USE ONLY FIRST   SS JAN86 01020000
         LA    R1,1(,R1)           PLUS 1                      SS JAN86 01030000
         STH   R1,SPFTFED          SET LENGTH OF SEARCH        SS JAN86 01040000
         NI    SPFTFF,255-X'40'    TURN OFF POSITION           SS NOV85 01050000
         B     ISPFLGA1                                        SS NOV85 01060000
ISPFLG01 DS    0H                                              SS NOV85 01070000
         LH    R1,SPFTFED                                      SS NOV85 01080000
         SH    R1,SPFPRLN          ALLOW FOR LENG OF STRING    SS NOV85 01090000
         BM    ISPFLGX3            TO LONG                     SS NOV85 01100000
         BZ    ISPFLGC1                                        SS NOV85 01110000
         LA    R1,1(,R1)                                       SS NOV85 01120000
         STH   R1,SPFTFED          SET LENGTH                  SS NOV85 01130000
         B     ISPFLGA1                                        SS NOV85 01140000
ISPFLGC1 DS    0H                                              SS NOV85 01150000
         OI    SPFTFF,X'40'        USE POSITION                SS NOV85 01160000
         B     ISPFLGA1                                        SS NOV85 01170000
ISPFLGX1 DS    0H                                              SS NOV85 01180000
         MVC   SPFMSGID,ISPPDSM SETUP MESSAGE                  SS NOV85 01190000
         MVI   SPFMSGID+7,C'A'                                 SS NOV85 01200000
         MVI   SPFMSGID+6,C'5'     RANGE ERROR                 SS NOV85 01210000
         B     ISPFLGX9            RETURN WITH ERROR           SS DEC87 01220000
ISPFLGX2 DS    0H                                              SS NOV85 01230000
         MVC   SPFMSGID,ISPPDSM SETUP MESSAGE                  SS NOV85 01240000
         MVI   SPFMSGID+7,C'A'                                 SS NOV85 01250000
         MVI   SPFMSGID+6,C'7'     CONFLICT ERROR              SS NOV85 01260000
         B     ISPFLGX9            RETURN WITH ERROR           SS DEC87 01270000
ISPFLGX3 DS    0H                                              SS NOV85 01280000
         MVC   SPFMSGID,ISPPDSM SETUP MESSAGE                  SS NOV85 01290000
         MVI   SPFMSGID+7,C'A'                                 SS NOV85 01300000
         MVI   SPFMSGID+6,C'3'     OPERAND LENGTH ERROR        SS NOV85 01310000
         B     ISPFLGX9            RETURN WITH ERROR           SS DEC87 01320000
ISPFLGX9 DS    0H                       *                      SS DEC87 01330000
         LH    R15,SPFTBPFD        FIND INDEX                  SS OCT88 01340000
         SLL   R15,1               TIMES 1 FOR OFFSET          SS OCT88 01350000
         LA    R0,ISPFLGXX(R15)         *                      SS OCT88 01360000
         LA    R15,168             ERROR PROMPT PANEL          SS DEC87 01370000
         BR    R3                  RETURN TO ROUTER            SS DEC87 01380000
ISPFLGA1 DS    0H                                              SS NOV85 01390000
         LH    R2,SPFPRLN          LENGTH OF STRING            SS NOV85 01400000
         CH    R2,ISPFLGLM         CHECK FOR LIMIT             SS OCT88 01410000
         BH    ISPFLGX3            TOO LONG                    SS OCT88 01420045
         CH    R2,ISPFZERO         CHECK FOR ZERO             DRK DEC09 01420148
         BE    ISPFLGX3            TOO SHORT, TAKE LONG ROAD  DRK DEC09 01421046
         STH   R2,SPFTFDL                                      SS NOV85 01430000
         LA    R1,SPFFDSAV                                     SS NOV85 01440000
         ST    R1,SPFTFDS          SAVE COMPARE STG ADDR       SS NOV85 01450000
         LH    R1,SPFPRLN                                      SS MAY85 01460000
         L     R15,SPFPRSR                                     SS MAY85 01470000
         MVI   SPFFDSAV,C' '       BLANK FIND VARIABLE        DRK DEC02 01471021
         MVC   SPFFDSAV+1(31),SPFFDSAV  RIPPLE                DRK DEC02 01472021
         EX    R1,ISPFLGMV         EXECUTE MVC CHAR            SS MAY85 01480000
         TM    SPFPRBI,X'80'       ASIS                        SS MAY85 01490000
         BO    ISPFLG04            DO NOT FORCE UPPER CASE     SS MAY90 01500000
         MVI   SPFINPUT,C' '       SETUP BLANK LINE            SS MAY85 01510000
         MVC   SPFINPUT+1(79),SPFINPUT  RIPPLE                ABL APR86 01520000
         LH    R1,SPFTFDL          FIND STRING LENGTH          SS NOV85 01530000
         EX    R1,ISPFLGOC         EXECUTE OR CHAR             SS MAY85 01540000
         B     ISPFLG04                                        SS NOV85 01550000
ISPFLGOC OC    SPFFDSAV,SPFINPUT   FORCE UPPER CASE            SS MAY85 01560000
ISPFLGMV MVC   SPFFDSAV(0),0(R15)  SAVE COMPARE STRING         SS NOV85 01570000
ISPFLG03 DS    0H                                              SS NOV85 01580000
         OI    SPFTFF2,X'08'       REPEAT FIND                 SS NOV85 01590000
ISPFLG04 BAS   R2,ISPFNDI                                      SS NOV85 01600000
ISPFIND2 DS    0H                                              SS NOV85 01610000
         LA    R15,100              ROUTE TO TBDISPLAY         SS NOV85 01620000
         B     ISPROUT              RETURN TO ROUTER           SS NOV85 01630000
ISPFLGLM DC    H'31'                MAX STRING LENGTH          SS OCT88 01640000
ISPFZERO DC    H'-1'                ZERO STRING LENGTH        DRK DEC09 01641048
         DROP  R4                                              SS NOV85 01650000
         SPACE 2                                                        01660000
ISPFDPR  DS    0F                                              SS NOV85 01670000
         DC    A(ISPPARFD)     0   PARSE LIST ADDRESS          SS NOV85 01680000
         DC    A(ISPPARF2)     4   MEMLIST SOURCE              SS NOV85 01690000
         DC    A(ISPPARF3)     8   MEMLIST LOAD MOD            SS NOV85 01700000
         DC    A(ISPPARF4)    12   LISTA                       SS NOV85 01710000
         DC    A(ISPPARF5)    16   LISTV                       SS NOV85 01720000
         DC    A(ISPPARF6)    20   LISTC/LISTF                 SS OCT88 01730000
         DC    A(ISPPARF7)    24   TCT                         SS OCT88 01740000
         DC    A(ISPPARF8)    28   CAX                         SS OCT88 01750000
         DC    A(ISPPARF9)    32   FILTER                      SS OCT88 01760000
         DC    A(ISPPARFA)    36   CSECT                       SS OCT88 01770000
ISPFDCK  DS    0F                                              SS NOV85 01780000
         DC    A(0)                TABLE ADDRESSES             SS NOV85 01790000
         DC    A(ISPFDCK2)                                     SS NOV85 01800000
         DC    A(ISPFDCK3)                                     SS NOV85 01810000
         DC    A(ISPFDCK4)                                     SS NOV85 01820000
         DC    A(ISPFDCK5)                                     SS NOV85 01830000
         DC    A(ISPFDCK6)                                     SS OCT88 01840000
         DC    A(ISPFDCK7)                                     SS OCT88 01850000
         DC    A(ISPFDCK8)                                     SS OCT88 01860000
         DC    A(ISPFDCK9)                                     SS OCT88 01870000
         DC    A(ISPFDCKA)                                     SS OCT88 01880000
ISPFLGXX DS    0H                PROMPT PANEL                  SS OCT88 01890000
         DC    CL8'&P.OPFTS'   0   LOG FIND PROMPT             SS OCT88 01900000
         DC    CL8'&P.MNFTM'   4   MEMLIST SOURCE              SS OCT88 01910000
         DC    CL8'&P.MNFTU'   8   MEMLIST LOAD MOD            SS OCT88 01920000
         DC    CL8'&P.LAFTA'  12   LISTA                       SS OCT88 01930000
         DC    CL8'&P.LVFTV'  16   LISTV                       SS OCT88 01940000
         DC    CL8'&P.LCFTC'  20   LISTC/LISTF                 SS OCT88 01950000
         DC    CL8'&P.TTFTT'  24   TCT                         SS OCT88 01960000
         DC    CL8'&P.CXFTX'  28   CAX                         SS OCT88 01970000
         DC    CL8'&P.FLFTF'  32   FILTER                      SS OCT88 01980000
         DC    CL8'&P.CSFTS'  36   CSECT                       SS OCT88 01990000
* PARSE PARM LIST FOR FIND                                     SS MAR85 02000000
         DS    0F                                    MATCHES   SS MAR85 02010000
***                                                  ========= SS MAR85 02020000
ISPPARFD DC    X'80',X'00',X'00',X'00',CL8' ' STRING           SS MAY85 02030000
         DC    X'51',X'40',X'00',X'00',F'1',F'158' NUMERIC     SS OCT88 02040000
         DC    X'51',X'00',X'80',X'00',F'2',F'158' NUMERIC2    SS OCT88 02050000
         DC    X'31',X'80',X'00',X'00',CL8'ASIS'     A/AS     DRK DEC02 02060020
         DC    X'34',X'20',X'00',X'00',CL8'FIRST'    F/FIRST   SS MAY85 02070000
