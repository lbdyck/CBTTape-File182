         TITLE 'ISPF - FIND   LEVEL 1 SUBROUTINE'              SS NOV85 04620000
*  FIND STRING IN TABLE                                        SS MAY85 04630000
ISPFND   DS    0H                                              SS NOV85 04640000
         USING ISPFND,R4                                       SS MAY85 04650000
         BAS   R2,ISPQUERY                                     SS NOV85 04660000
         TM    SPFTBPF1,SPFTLOG    THIS LOG                    SS NOV85 04670000
         BZ    ISPFLG02                                        SS NOV85 04680000
         MVC   SPFTTOP,SPFSTART    SAVE START /TOP             SS MAY85 04690000
         MVC   SPFTTOP2,SPFTTOP    SAVE HERE ALSO              SS JUL87 04700000
         MVC   SPFTSIZE,SPFLAST    SAVE BOTTOM/SIZE            SS NOV85 04710000
         MVC   SPFTCRP,SPFCRP      SAVE CURRENT                SS NOV85 04720000
ISPFLG02 DS    0H                                              SS MAY85 04730000
         TM    SPFTFF2,X'08'       REPEAT FIND                 SS NOV85 04740000
         BO    ISPFLG05                                        SS NOV85 04750000
         MVC   SPFTCCRP,SPFTCRP    INIT TO CURRENT             SS NOV85 04760000
         MVC   SPFTFTOP,SPFTTOP    INIT CURRENT PLACE          SS APR86 04770000
         TM    SPFTFF,X'10'        BACKWARD SEARCH             SS MAY85 04780000
         BO    ISPFLG60                                        SS MAY85 04790000
         CLC   SPFTFTOP,ISPL1      AT TOP                      SS MAY85 04800000
         BNE   ISPFLG10                                        SS MAY85 04810000
         OI    SPFTFF2,X'02'       FROM TOP                    SS MAY85 04820000
         B     ISPFLG10                                        SS MAY85 04830000
ISPFLG05 DS    0H                                              SS MAY85 04840000
         MVI   SPFINPUT,C' '       SETUP BLANK LINE            SS MAY85 04850000
         MVC   SPFINPUT+1(157),SPFINPUT RIPPLE                 SS OCT88 04860000
         L     R2,SPFTFDS          STING ADDRESS               SS NOV85 04870000
         CLI   0(R2),0             ANY STRING                  SS NOV85 04880000
         BE    ISPFLGEM            CANNOT DO REPEAT FIND       SS MAY85 04890000
         TM    SPFTFF2,X'01'       HAVE STRING                 SS MAY85 04900000
         BZ    ISPFLGEM            NO, NO REPEAT FIND          SS MAY85 04910000
         TM    SPFTFF,X'10'        BACKWARD SEARCH             SS MAY85 04920000
         BO    ISPFLG50            START HERE                  SS MAY85 04930000
         CLC   SPFTFTOP,SPFTTOP    STILL SAME PLACE            SS MAY85 04940000
         BNE   ISPFLG07                                        SS MAY85 04950000
         TM    SPFTFF,X'20'        FIRST KEY WORD              SS MAY85 04960000
         BO    ISPFLG15            NO, THEN USE CURRENT        SS MAY85 04970000
         TM    SPFTBPF1,SPFTLOG    THIS LOG                    SS OCT88 04980000
         BO    ISPFLG28            YES, THEN RESTART AT HERE   SS OCT88 04990000
         L     R15,SPFTTOP         SET NEW PLACE               SS MAY85 05000000
         LA    R15,1(,R15)                                     SS MAY85 05010000
         C     R15,SPFTSIZE        BOTTOM                      SS MAY85 05020000
         BH    ISPFLGEN            RAN OFF BOTTOM              SS MAY85 05030000
         ST    R15,SPFTTOP                                     SS MAY85 05040000
         ST    R15,SPFTTOP2        SAVE HERE ALSO              SS JUL87 05050000
         B     ISPFLG30                                        SS MAY85 05060000
ISPFLG07 DS    0H                                              SS MAY85 05070000
         MVC   SPFTFTOP,SPFTTOP    INIT CURRENT PLACE          SS APR86 05080000
         NI    SPFTFF,255-X'20'    RESET FIRST                 SS MAY85 05090000
ISPFLG10 DS    0H                  PROCESS FIND                SS MAY85 05100000
         TM    SPFTFF,X'20'        FIRST KEY WORD              SS MAY85 05110000
         BZ    ISPFLG30            NO, THEN USE CURRENT        SS MAY85 05120000
ISPFLG15 DS    0H                  GO TO TOP                   SS MAY85 05130000
         NI    SPFTFF,255-X'20'    RESET FIRST                 SS MAY85 05140000
         OI    SPFTFF2,X'02'       FROM TOP                    SS MAY85 05150000
         BAS   R2,ISPTOP           START AT TOP                SS MAY85 05160000
         LA    R15,1                                           SS MAY85 05170000
         ST    R15,SPFTCRP         NOW AT TOP                  SS MAY85 05180000
         BAS   R2,ISPSKIP                                      SS MAY85 05190000
         LTR   R15,R15                                         SS MAY85 05200000
         BNZ   ISPFLGEN            RAN OFF BOTTOM              SS MAY85 05210000
         B     ISPFLG40                                        SS MAY85 05220000
ISPFLG28 DS    0H                                              SS OCT88 05230000
         L     R15,SPFTTOP         START SEARCH HERE           SS OCT88 05240000
         S     R15,SPFTCRP         RELATIVE POSITION           SS OCT88 05250000
         BAS   R2,ISPSKIP          POSITION SUBR               SS OCT88 05260000
