         TITLE 'ISPF - OUTPUT LV 1 SUBROUTINE'                          05958000
***  EXTRACT THE CURRENT ISPMODE/MEMLIST TABLE                ABL JUN85 05959000
ISPDISPT DS    0H                                             ABL JUN85 05960000
         USING *,R4                                                     05961000
         MVC   SPFMSGID(8),ISPBLK8   TURN OFF MESSAGES                  05962000
         MVC   SPFCURSR(8),ISPBLK8   TURN OFF CURSOR NAME               05963000
         NI    HFLAGSDD,FF-X'02'     NO OPEN YET                        05964000
ISPD020  OI    ##ADRPA#,$A+$D        IGNORE ATTENTION NOW               05965000
         NI    HFLAGSDD,FF-X'01'     NO ATTRIBUTES                      05966000
         BAS   R2,REFCHECK           CONDITIONAL REFRESH      ABL JAN87 05967000
         LA    R14,ISPDISP           DISPLAY TABLE                      05968000
         LA    R15,ISPDEXN           PANEL NAME                         05969000
         LA    R0,ISPBLK8            NO MSG                    SS NOV86 05970000
         CLC   SPFMSGID(4),ISPPDSM   ANY MSG                            05971000
         BNE   ISPD030                                                  05972000
         LA    R0,SPFMSGID                                              05973000
*  ALWAYS SPECIFY PANEL ID              *                      SS DEC87 05974000
*        CLC   SPFPANPR(8),0(R15)    SAME PANEL AS LAST TIME?           05975000
*        BNE   ISPD030               NO, BRANCH                         05976000
*        LA    R15,ISPBLK8           YES, NULLIFY PANEL NAME   SS NOV86 05977000
ISPD030  MVC   SPFPANPR(8),ISPDEXN   SAVE LAST PANEL NAME               05978000
         LA    R1,SPFCURSR           DEFAULT CURSOR POSITIONING         05979000
         STM   R14,R1,PARMLIST                                          05980000
         OI    PARMLIST+12,X'80'                                        05981000
         LA    R1,PARMLIST                                              05982000
         L     R15,ISPLINK                                              05983000
