ISPD090  CLI   SPFDDEST,X'41'                    ANY DEST?              06133000
         BL    ISPD095                           NO, BRANCH             06134000
         LA    R15,4(,R15)                       NEXT POINTER ELEMENT   06135000
         MVC   0(L'ISPDDEST,R2),ISPDDEST         SPECIFY DEST TEXT      06136000
         MVC   6(8,R2),SPFDDEST                  SPECIFY DESTINATION    06137000
         ST    R2,S99TUPTR                       POINT TO TEXT UNIT     06138000
         LA    R2,L'ISPDDEST(,R2)                START OF NEXT TEXT     06139000
         SPACE 1                                                        06140000
ISPD095  CLC   SPFDNUM(2),=C'01'                 ANY COPY NUMBER        06141000
         BE    ISPD100                           NO, BRANCH             06142000
         LA    R15,4(,R15)                       NEXT POINTER ELEMENT   06143000
         MVC   0(L'ISPDNUMC,R2),ISPDNUMC         SPECIFY COPIES TEXT    06144000
         CLI   SPFDNUM+1,X'40'                   SECOND DIGIT BLANK?    06145000
         BH    ISPD096                           NO, BRANCH             06146000
         MVC   SPFDNUM+1(1),SPFDNUM              SLIDE FIRST DIGIT DOWN 06147000
         MVI   SPFDNUM,C'0'                      ADD A LEADING ZERO     06148000
ISPD096  PACK  DOUBLE(8),SPFDNUM(2)              CONVERT TO PACKED      06149000
         CVB   R0,DOUBLE                         CONVERT TO BINARY      06150000
         STC   R0,6(,R2)                         SPECIFY THE COUNT      06151000
         ST    R2,S99TUPTR                       POINT TO TEXT UNIT     06152000
         LA    R2,L'ISPDNUMC(,R2)                START OF NEXT TEXT     06153000
         SPACE 1                                                        06154000
ISPD100  LA    R15,4(,R15)                       NEXT POINTER ELEMENT   06155000
         MVC   0(L'ISPDSYSO,R2),ISPDSYSO         SPECIFY SYSOUT TEXT    06156000
         MVC   6(1,R2),SPFDSOUT                  SPECIFY SYSOUT CLASS   06157000
         ST    R2,S99TUPTR                       POINT TO TEXT UNIT     06158000
         SPACE 1                                                        06159000
ISPD110  OI    S99TUPTR,X'80'                    MARK END OF LIST       06160000
         DYNALLOC                                                       06161000
         SPACE 2                                                        06162000
         DROP  R1,R14,R15                                               06163000
         MVC   DDNAMEH,WORKTBL+6        SAVE DDNAME                     06164000
         LTR   R15,R15                  SUCCESSFUL?                     06165000
         BZ    ISPD120                  YES, BRANCH                     06166000
         SPACE 2                                                        06167000
         LA    R14,M99RB                POINTER TO SVC 99 BLOCK         06168000
         ST    R15,DAIRRC               RETURN CODE                     06169000
         LA    R15,DAIRRC               POINTER TO RETURN CODE          06170000
         LA    R0,ADDRFF02              POINTER TO A(IKJEFF02)          06171000
         LA    R1,=AL2(DFSVC99)         POINTER TO INVOCATION TYPE      06172000
         L     R2,ADDRCPPL              POINTER TO THE CPPL             06173000
         STM   R14,R2,DFDAPLP           INITIALIZE DFDAPLP, DFDRCP,     06174000
*                                         DFJEFF02, DFIDP AND DFCPPLP   06175000
         SPACE 1                                                        06176000
         LINK  EP=IKJEFF18,MF=(E,DFPARMS)                               06177000
         MVC   SPFMSGID(8),ISPD701X     ERROR MESSAGE                   06178000
         LTR   R15,R15                  PROBLEM WITH IKJEFF18?          06179000
         BNZ   ISPD020                  YES, BRANCH                     06180000
         B     ISPD020                                                  06181000
         SPACE 2                                                        06182000
ISPD120  CLI   SPFDSOUT,X'40'           SYSOUT ALLOCATION?              06183000
         BH    ISPD180                  YES, BRANCH                     06184000
         CLI   WORKTBL+14+6,DS1DSGPO    PARTITIONED?                    06185000
         BNE   ISPD130                  NO, BRANCH                      06186000
         CLI   SPFDMEM,X'41'            ANY MEMBER NAME?                06187000
         BL    ISPD170                  NO, ERROR                       06188000
         B     ISPD180                                                  06189000
         SPACE 1                                                        06190000
ISPD130  CLI   WORKTBL+14+6,DS1DSGPS    SEQUENTIAL?                     06191000
         BNE   ISPD170                  NO, ERROR                       06192000
         CLI   SPFDMEM,X'41'            ANY MEMBER NAME?                06193000
         BL    ISPD180                  NO, OK                          06194019
         SPACE 1                                                        06195000
ISPD170  MVC   SPFMSGID(8),ISPD702X     ERROR MESSAGE                   06196000
         B     ISPD020                                                  06197000
         SPACE 2                                                        06198000
*** OPEN THE SYSOUT/DATA SET                                            06199000
ISPD180  MVC   BAMDCB(ISPDDCBL),ISPDDCB      CONSTRUCT A DCB            06200000
         MVC   DCBDDNAM-IHADCB+BAMDCB(8),DDNAMEH                        06201000
         MVI   OPENLIST,X'80'                                           06202000
         OPEN  (BAMDCB,(OUTPUT)),MF=(E,OPENLIST)                        06203000
         TM    DCBOFLGS-IHADCB+BAMDCB,X'10'  OPEN ALREADY               06204000
         BNO   ISPD190                       NO, ERROR                  06205000
         OI    HFLAGSDD,X'02'                OPEN NOW                   06206000
         TM    HFLAGSDD,X'01'                OPEN WITH GOOD VALUES?     06207000
         BO    ISPD200                       YES, BRANCH                06208000
         MVI   OPENLIST,X'80'                                           06209000
         CLOSE (BAMDCB),MF=(E,OPENLIST)                                 06210000
ISPD190  MVC   SPFMSGID(8),ISPD703X     ERROR MESSAGE                   06211000
         B     ISPD020                                                  06212000
         SPACE 2                                                        06213000
*** OUTPUT THE DATA                                                     06214000
ISPD200  DS    0H                                                       06215000
         BAS   R2,ISPMVTBL              GET INFO               SS AUG86 06216000
         LA    R15,4(,R15)              ALTERNATE ENTRY        SS AUG86 06217000
         ST    R15,SPFTSV1              TEMPORARY SAVE ADDRESS SS AUG86 06218017
         ST    R14,SPFTSV2              ALSO SAVE TITLE ADDR   SS AUG86 06219000
         MVC   SPFTSVX,10(R1)           ALSO SAVE TITLE COUNT  SS AUG86 06220000
         MVC   SPFTSVC,11(R1)           ALSO SAVE HEADER TYPE ABL FEB87 06221000
         SR    R0,R0                    *                      SS AUG86 06222000
         IC    R0,9(R1)                 GET LENGTH             SS AUG86 06223000
         STH   R0,SPFTSVH               SAVE LENGTH            SS AUG86 06224000
*                                                                       06225000
         STM   R14,R8,MSGTEXT1              SAVE ENTRY REGS   ABL FEB87 06226000
         L     R8,ISPDTIME                  TIME ROUTINE      ABL FEB87 06227000
         MVI   ##SUBCOM,C'N'                NOT "TIME"        ABL FEB87 06228000
         BASR  R2,R8                        CALL TIME ROUTINE ABL FEB87 06229000
         LM    R14,R8,MSGTEXT1              RESET ENTRY REGS  ABL FEB87 06230000
*                                                                       06231000
         MVI   MSGTEXT1+4,X'40'                               ABL FEB87 06232000
         MVC   MSGTEXT1+5(255),MSGTEXT1+4   CLEAR MSGTEXT1    DRK APR08 06233041
*        MVC   MSGTEXT1+5(79),MSGTEXT1+4    CLEAR MSGTEXT1    ABL FEB87 06233130
         SR    R1,R1                                          ABL FEB87 06234000
         IC    R1,SPFTSVC                   HEADER TYPE       ABL FEB87 06235000
         SLL   R1,5                         TIMES 32          ABL FEB87 06236000
