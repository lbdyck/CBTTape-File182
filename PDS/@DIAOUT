         BAS   R2,ISPDVCOP           COPY INTO STORAGE                  06023000
         SPACE 3                                                        06024000
         LA    R1,M99RBPTR              DYNAMIC ALLOCATION              06025000
         XC    M99RBPTR(40),M99RBPTR    CLEAR THE WORK AREA             06026000
         LA    R1,M99RBPTR              DYNAMIC ALLOCATION              06027000
         USING S99RBP,R1                                                06028000
         SPACE 1                                                        06029000
         LA    R14,M99RB                                                06030000
         USING S99RB,R14                                                06031000
         ST    R14,S99RBPTR                                             06032000
         OI    S99RBPTR,S99RBPND        MARK END OF LIST                06033000
         SPACE 1                                                        06034000
